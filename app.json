{
  "expo": {
    "name": "Self blog",
    "slug": "rn-self-posts",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/r-icon.png",
    "splash": {
      "image": "./assets/r-image.png",
      "resizeMode": "cover",
      "backgroundColor": "#242832"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kobevko.self-blog-rn-app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.kobevko.self_blog_rn_app",
      "versionCode": 1,
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE"]
    }
  }
}
